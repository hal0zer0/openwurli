+  NR=1.005       VAR=26       IKR=0.01    ISC=1.7E-13
+  NC=1.214       RB=120       IRB=0       RBM=120
+  RE=0.5         RC=2         CJE=3.33E-12 VJE=0.65
+  MJE=0.33       TF=7.2E-10   XTF=8       VTF=2
+  ITF=0.08       PTF=0        CJC=1.66E-12 VJC=0.4
+  MJC=0.33       XCJC=0.5     TR=1E-07    CJS=0
+  VJS=0.75       MJS=0.33     XTB=1.4     EG=1.11
+  XTI=3          KF=5E-15     AF=1        FC=0.5)

